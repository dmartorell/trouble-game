{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dmartorell.troublegame",
      "statusBar": {
        "hidden": true
      }
    },
    "icon": "./assets/icon.png",
    "ios": {
      "bundleIdentifier": "com.dmartorell.troublegame",
      "supportsTablet": true,
      "infoPlist": {
        "UIStatusBarHidden": true,
        "UIViewControllerBasedStatusBarAppearance": false
      }
    },
    "name": "trouble-game",
    "newArchEnabled": true,
    "orientation": "portrait",
    "plugins": [
      "expo-router"
    ],
    "scheme": "troublegame",
    "slug": "trouble-game",
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain"
    },
    "userInterfaceStyle": "light",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
